{
  "name": "horizon-simulations-fbw",
  "version": "2.0.0",
  "description": "Horizon Simulations FlyByWire Compatibility Core Repository",
  "scripts": {
    "====== A318ceo =================": "==========================================",

    "build-a318ceo:copy-base-package": "mkdir -p build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo && (rsync -a hsim-a318ceo/src/base/lvfr-horizonsim-airbus-a318-ceo build-a318ceo/out/ || cp -a -u hsim-a318ceo/src/base/lvfr-horizonsim-airbus-a318-ceo build-a318ceo/out/)",
    "build-a318ceo:copy-base-lock-highlight-package": "mkdir -p build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo-lock-highlight && (rsync -a hsim-a318ceo/src/base/lvfr-horizonsim-airbus-a318-ceo-lock-highlight build-a318ceo/out/ || cp -a -u hsim-a318ceo/src/base/lvfr-horizonsim-airbus-a318-ceo-lock-highlight build-a318ceo/out/)",
    "build-a318ceo:copy-base-files": "npm run build-a318ceo:copy-base-package && npm run build-a318ceo:copy-base-lock-highlight-package",
    "build-a318ceo:efb-translation": "cd hsim-a318ceo/src/localization && node build-flypad-translation.js",
    "build-a318ceo:locPak-translation": "cd hsim-a318ceo/src/localization && node build-locPak-translation.js",

    "build-a318ceo:behavior": "node build-a318ceo/src/behavior/build.js",

    "build-a318ceo:atsu-common": "rollup -c build-a318ceo/src/systems/atsu/common/build.js",
    "build-a318ceo:atsu-fms-client": "node build-a318ceo/src/systems/atsu/fmsclient/build.js",
    "build-a318ceo:extras-host": "cd build-a318ceo/src/systems/extras-host && rollup -c",
    "build-a318ceo:failures": "rollup -c build-a318ceo/src/systems/failures/rollup.config.js",
    "build-a318ceo:fmgc": "rollup -c build-a318ceo/src/systems/fmgc/rollup.config.js",
    "build-a318ceo:instruments": "mach build --config build-a318ceo/mach.config.js --work-in-config-dir",
    "build-a318ceo:dev_instruments": "mach watch --config build-a318ceo/mach.config.js --work-in-config-dir",
    "build-a318ceo:sentry-client": "rollup -c build-a318ceo/src/systems/sentry-client/rollup.config.js",
    "build-a318ceo:simbridge-client": "rollup -c build-a318ceo/src/systems/simbridge-client/rollup.config.js",
    "build-a318ceo:systems-host": "cd build-a318ceo/src/systems/systems-host && rollup -c",
    "build-a318ceo:tcas": "rollup -c build-a318ceo/src/systems/tcas/rollup.config.js",

    "build-a318ceo:fadec-cfm": "cd build-a318ceo/src/wasm/fadec_a320 && ./build.sh && wasm-opt -O1 -o /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/fadec.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/fadec.wasm",
    "build-a318ceo:fbw-cfm": "cd build-a318ceo/src/wasm/fbw_a320 && ./build.sh && wasm-opt -O1 -o /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/fbw.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/fbw.wasm",
    "build-a318ceo:flypad-backend-cfm": "cd build-a318ceo/src/wasm/flypad-backend && ./build.sh && wasm-opt -O1 -o /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/flypad-backend.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/flypad-backend.wasm",
    "build-a318ceo:systems-cfm": "cargo build -p a320_systems_wasm --target wasm32-wasi --release && wasm-opt -O1 -o /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/systems.wasm /external/target/wasm32-wasi/release/a320_systems_wasm.wasm",

    "build-a318ceo:fadec-cfm-cj": "cp -a -u /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/fadec.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318cjCFM/panel/fadec.wasm",
    "build-a318ceo:fbw-cfm-cj": "cp -a -u /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/fbw.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318cjCFM/panel/fbw.wasm",
    "build-a318ceo:flypad-backend-cfm-cj": "cp -a -u /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/flypad-backend.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318cjCFM/panel/flypad-backend.wasm",
    "build-a318ceo:systems-cfm-cj": "cp -a -u /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318ceoCFM/panel/systems.wasm /external/build-a318ceo/out/lvfr-horizonsim-airbus-a318-ceo/SimObjects/Airplanes/A318cjCFM/panel/systems.wasm",

    "build-a318ceo:metadata": "node scripts/metadata.js build-a318/out/lvfr-horizonsim-airbus-a318-ceo A318HS",
    "build-a318ceo:manifest": "node scripts/build-a318hs.js",

    "build-a318ceo:ace": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c .\\build-a318ceo\\src\\systems\\instruments\\buildSrc\\aceBuild.mjs",
    "watch-a318ceo:ace": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -wc .\\build-a318ceo\\src\\systems\\instruments\\buildSrc\\aceBuild.mjs",

    "build-a318ceo:all": "npm run build-a318ceo:copy-base-files && npm run build-a318ceo:efb-translation && npm run build-a318ceo:behavior && npm run build-a318ceo:atsu-common && npm run build-a318ceo:atsu-fms-client && npm run build-a318ceo:systems-host && npm run build-a318ceo:failures && npm run build-a318ceo:fmgc && npm run build-a318ceo:sentry-client && npm run build-a318ceo:simbridge-client && npm run build-a318ceo:tcas && npm run build-a318ceo:pfd && npm run build-a318ceo:systems && npm run build-a318ceo:fadec-cfm && npm run build-a318ceo:fbw-cfm && npm run build-a318ceo:flypad-backend-cfm && npm run build-a318ceo:fadec-cfm-cj && npm run build-a318ceo:fbw-cfm-cj && npm run build-a318ceo:flypad-backend-cfm-cj && npm run build-a318ceo:metadata && npm run build-a318ceo:manifest",

    "prettier": "prettier --write **/*.json **/*.yml build-a318ceo/src/systems/instruments/**/*.css",
    "serve:efb": "cd build-a318ceo/src/systems/instruments/src/EFB/ && vite --port 9696",
    "build:instruments": "rollup --max-old-space-size=8192 -c src/systems/instruments/buildSrc/simulatorBuild.mjs",
    "watch:instruments": "rollup --max-old-space-size=8192 -wc src/systems/instruments/buildSrc/simulatorBuild.mjs",

    "====== COMMON ================": "==========================================",
    "lint": "eslint --cache **/*.{js,mjs,jsx,ts,tsx}",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,mjs,jsx}": "eslint --cache --fix",
    "*.{json,yml,css,scss}": "prettier --write"
  },
